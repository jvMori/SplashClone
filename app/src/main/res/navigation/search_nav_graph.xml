<navigation android:id="@+id/searchFragment"
            app:startDestination="@id/searchFragment"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <fragment android:id="@+id/searchFragment"
              android:name="com.jvmori.myapplication.search.presentation.ui.fragments.SearchFragment"
    >
        <action android:id="@+id/action_searchFragment_to_searchPhotosFragment"
                app:destination="@id/searchPhotosFragment"/>
        <action android:id="@+id/action_searchFragment_to_searchCollectionsFragment"
                app:destination="@id/searchCollectionsFragment"/>
        <action android:id="@+id/action_searchFragment_to_searchUsersFragment"
                app:destination="@id/searchUsersFragment"/>
        <action android:id="@+id/action_searchFragment_to_collectionsPhotos" app:destination="@id/collectionsPhotos">
            <argument android:defaultValue="-1" android:name="collectionId"/>
        </action>
    </fragment>
    <fragment android:id="@+id/searchPhotosFragment"
              android:name="com.jvmori.myapplication.search.presentation.ui.fragments.SearchPhotosFragment"
              android:label="fragment_search_photos" tools:layout="@layout/fragment_search_photos"/>
    <fragment android:id="@+id/searchCollectionsFragment"
              android:name="com.jvmori.myapplication.search.presentation.ui.fragments.SearchCollectionsFragment"
              android:label="SearchCollectionsFragment">
    </fragment>
    <fragment android:id="@+id/searchUsersFragment"
              android:name="com.jvmori.myapplication.search.presentation.ui.fragments.SearchUsersFragment"
              android:label="SearchUsersFragment"/>
    <fragment android:id="@+id/collectionsPhotos"
              android:name="com.jvmori.myapplication.photoslist.presentation.ui.PhotosFragment"
              android:label="PhotosFragment">
        <argument android:name="collectionId" app:argType="integer" android:defaultValue="-1"/>
    </fragment>
</navigation>